{
  "extends": [
    "config:base"
  ],
  "registryUrls": ["https://eh3qxbpvoc.execute-api.us-east-1.amazonaws.com/default"],
  "enabledManagers": ["regex", "docker"],
  "regexManagers": [
    {
      "fileMatch": ["\\.tf$", "^terragrunt.hcl$"],
      "matchStrings": ["source\\s*=\\s*\"github\\.com.gruntwork-io/(?:(?<prefix>terraform)-(?<cloud>.+?)-)?(?<name>.+?)(?:/.*?)?\\?ref=(?<currentValue>.*?)\""],
      "depNameTemplate": "gruntwork-io/{{name}}/{{#if cloud}}{{cloud}}{{else}}aws{{/if}}",
      "datasourceTemplate": "terraform-module"
    }
  ],
  "prBodyColumns": [
    "Package",
    "Update",
    "Type",
    "New value",
    "Release Notes"
  ],
  "prBodyDefinitions": {
    "Release Notes": "https://github.com/gruntwork-io/{{name}}/releases/tag/{{newVersion}}"
  }
}
